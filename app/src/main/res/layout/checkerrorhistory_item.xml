<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:orientation="vertical" android:layout_width="match_parent"
    android:background="@drawable/classfragmentlistview_back"
    android:layout_height="wrap_content">
    <LinearLayout
        android:orientation="horizontal"
        android:layout_width="match_parent"
        android:layout_height="wrap_content">
        <TextView
            android:id="@+id/checkerrorhistoryitem_text_dataStr"
            android:text="2020年4月26日"
            android:layout_width="0dp"
            android:textColor="#000000"
            android:textSize="15dp"
            android:padding="10dp"
            android:layout_weight="1"
            android:layout_height="wrap_content" />
        <TextView
            android:id="@+id/checkerrorhistoryitem_text_errorSize"
            android:text="错误题数：4"
            android:layout_weight="1"
            android:layout_width="0dp"
            android:textSize="15dp"
            android:textColor="#000000"
            android:padding="10dp"
            android:layout_height="wrap_content" />
    </LinearLayout>
    <LinearLayout
        android:background="#ffffff"
        android:id="@+id/checkerrorhistoryitem_linear_result"
        android:orientation="vertical"
        android:layout_width="match_parent"
        android:layout_height="wrap_content">
    </LinearLayout>
</LinearLayout>