<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:orientation="vertical" android:layout_width="match_parent"
    android:padding="5dp"
    android:background="@drawable/linear_background"
    android:layout_height="wrap_content">
    <TextView
        android:id="@+id/checkhistoryfragmentlistview_text_dataStr"
        android:text="2020年04月26日"
        android:textSize="18dp"
        android:textColor="#000000"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content" />
    <ImageView
        android:id="@+id/checkhistoryfragmentlistview_image_photoUrl"
        android:layout_marginTop="10dp"
        android:visibility="gone"
        android:layout_width="wrap_content"
        android:layout_height="200dp" />
    <LinearLayout
        android:orientation="horizontal"
        android:layout_width="match_parent"
        android:padding="5dp"
        android:layout_height="wrap_content">
        <TextView
            android:id="@+id/checkhistoryfragmentlistview_text_standardSize"
            android:text="答对5道"
            android:layout_marginTop="5dp"
            android:textColor="#000000"
            android:textSize="14dp"
            android:layout_weight="1"
            android:layout_width="0dp"
            android:layout_height="wrap_content" />
        <TextView
            android:id="@+id/checkhistoryfragmentlistview_text_errorSize"
            android:text="答错4道"
            android:textColor="#F44336"
            android:textSize="14dp"
            android:layout_marginTop="5dp"
            android:layout_width="0dp"
            android:layout_weight="1"
            android:layout_height="wrap_content" />
    </LinearLayout>
    <LinearLayout
        android:padding="10dp"
        android:orientation="vertical"
        android:id="@+id/checkhistoryfragmentlistview_linear_result"
        android:layout_width="match_parent"
        android:layout_height="wrap_content">

    </LinearLayout>
</LinearLayout>